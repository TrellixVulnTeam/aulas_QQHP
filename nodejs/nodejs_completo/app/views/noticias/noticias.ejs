<!DOCTYPE html>
	<html lang="pt-BR">
		<head>
			<meta charset="utf-8"/>
			<title>Portal de notícias</title>
		</head>
	<body>

		<h1>Notícia</h1>
		<table>
			<tr>
				<td>ID</td>
				<td>TITULO</td>
			</tr>

			<% for(var i = 0; i < noticias.length; i++) { %>
				
				<tr>
					<td><%= noticias[i].id_noticias %></td>
					<td><%= noticias[i].titulo %></td>
				</tr>

			<% } %>
		</table>
	</body>
</html>